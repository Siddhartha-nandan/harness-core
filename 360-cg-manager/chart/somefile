# Source: harness-manager/templates/external-secret.yaml
apiVersion: external-secrets.io/v1beta1
kind: ExternalSecret
metadata:
  name: harness-manager-ext-secret-0
spec:
  secretStoreRef:
    name:  a
    kind: a
  target:
    name: harness-manager-ext-secret-0
    template:
      engineVersion: v2
      mergePolicy: Replace
      data:
        REDIS_USERNAME: "{{ .redis_username }}"
  data:
  - secretKey: redis_username
    remoteRef:
      key: a
---
